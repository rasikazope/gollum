using namespace std;


void swap (int &a, int &b) {
    int tmp;
    tmp = a;
    a = b;
    b = tmp;
}


int
first_missing_pos_num(vector <int> arr)
{
    for (int i = 0; i < arr.size(); i++) {
        if (arr[i] < 0 && arr[i] > arr.size()) {
            continue;
        } else {
        }
    }
}


int main() {

    int vector <int> arr =  {1, 3, 5, 4};
    cout << first_missing_pos_num(arr) << endl;
    return 0;
}
